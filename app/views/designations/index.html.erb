<h1 align="center">List of All Designation</h1>
<div align="center">
  <%= will_paginate %>
  <% @designations.each do |desig| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{desig.name}", designation_path(desig) %>
          </li>
          <li><small></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <center><%= will_paginate %></center>
  <center><%= link_to "Create New Designation", new_designation_path %></center>
</div>