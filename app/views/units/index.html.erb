<h1 align="center">List of All Units</h1>
<div align="center">
  <%= will_paginate %>
  <% @units.each do |unit| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{unit.name}", unit_path(unit) %>
          </li>
          <li><small></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <center><%= will_paginate %></center>
  <center><%= link_to "Create New Unit", new_unit_path %></center>
</div>