<h1 align="center">List of All Assigned Officer</h1>
<div align="center">
  <%= will_paginate %>
  <% @officers.each do |off| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{off.user.empname}", officer_path(off) %>
          </li>
          <li><small></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <center><%= will_paginate %></center>
  <center><%= link_to "Assign Officer", new_officer_path %></center>
</div>