<h1 align="center">List of All Brands</h1>
<div align="center">
  <%= will_paginate %>
  <% @brands.each do |bra| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{bra.name}", brand_path(bra) %>
          </li>
          <li><small></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <center><%= will_paginate %></center>
  <center><%= link_to "Create New Brand", new_brand_path %></center>
</div>