<h1 align="center">List of All Categories</h1>
<div align="center">
  <%= will_paginate %>
  <% @suppliers.each do |supp| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{supp.name}", supplier_path(supp) %>
          </li>
          <li><small></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <center><%= will_paginate %></center>
  <center><%= link_to "New Supplier", new_supplier_path %></center>
</div>
