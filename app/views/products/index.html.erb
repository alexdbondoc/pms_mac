<h1 align="center">List of All Products</h1>
<div align="center">
  <%= will_paginate %>
  <% @products.each do |product| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{product.name}", product_path(product) %>
          </li>
          <li><small></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <center><%= will_paginate %></center>
  <center><%= link_to "Create New Product", new_product_path %></center>
</div>