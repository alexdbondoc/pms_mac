<h1 align="center">List of All Product Types</h1>
<div align="center">
  <%= will_paginate %>
  <% @types.each do |type| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="article-title">
            <%= link_to "#{type.name}", type_path(type) %>
          </li>
          <li><small></small></li>
        </div>
      </div>
    </ul>
  <% end %>
  <center><%= will_paginate %></center>
  <center><%= link_to "Create New Product Type", new_type_path %></center>
</div>